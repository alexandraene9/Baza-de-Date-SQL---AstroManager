----------------------------------------------------------------------------Creare tabele--------------------------------------------------------------
--1.  Departamente: –Creare tabel Departamente
CREATE TABLE Departamente(
    ID_Departament NUMBER PRIMARY KEY,
    Nume_Departament VARCHAR2(50) NOT NULL);

--2. Functii:  –Creare tabel Functii
CREATE TABLE Functii(
    ID_Functie NUMBER PRIMARY KEY,
    Nume_Functie VARCHAR2(50) NOT NULL);

--3. Angajati: –Creare tabel Angajati
CREATE TABLE Angajati( 
    ID_Angajat NUMBER PRIMARY KEY,
    Nume VARCHAR2(50) NOT NULL,
    Prenume VARCHAR2(50) NOT NULL, 
    ID_Departament NUMBER,
    ID_Functie NUMBER,
    Salariu NUMBER CHECK (Salariu>0),
    Data_Angajarii DATE,
   FOREIGN KEY (ID_Departament) REFERENCES Departamente(ID_Departament),
   FOREIGN KEY(ID_Functie) REFERENCES Functii(ID_Functie));
ALTER TABLE Angajati ADD ID_User NUMBER;
ALTER TABLE Angajati ADD CONSTRAINT FK_Angajati_User FOREIGN KEY (ID_User) REFERENCES Users(ID_User);

--4. Proiecte: –Creare tabel Proiecte
CREATE TABLE Proiecte(
ID_Proiect NUMBER PRIMARY KEY,
Nume_Proiect VARCHAR2(100) NOT NULL,
Buget NUMBER CHECK(Buget>0),
Data_Inceput DATE,
Data_Sfarsit DATE);
ALTER TABLE Proiecte ADD Status_Proiect VARCHAR2(50);

--5. Lansari: –Creare tabel Lansari
CREATE TABLE Lansari(
    ID_Lansare NUMBER PRIMARY KEY,
    ID_Proiect NUMBER REFERENCES Proiecte(ID_Proiect),
    Data_Lansare DATE,
    Cost NUMBER CHECK (Cost>0));

--6. Vehicule Spatiale: –Creare tabel Vehicule Spatiale
CREATE TABLE Vehicule_Spatiale(
    ID_Vehicul NUMBER PRIMARY KEY,
    Nume_Vehicul VARCHAR2(50) NOT NULL,
    Tip_Vehicul VARCHAR2(50),
    Data_Construirii DATE,
    ID_Lansare NUMBER REFERENCES Lansari(ID_Lansare));

--7. Clienti: –Creare tabel Clienti
CREATE TABLE Clienti(
    ID_Client NUMBER PRIMARY KEY,
    Nume_Client VARCHAR2(100) NOT NULL,
    Tara VARCHAR2(50),
    Email VARCHAR(100));

--8. Contracte: –Creare tabel Contracte
CREATE TABLE Contracte(
    ID_Contract NUMBER PRIMARY KEY,
    ID_Client NUMBER REFERENCES Clienti(ID_Client),
    ID_Proiect NUMBER REFERENCES Proiecte(ID_Proiect),
    Suma NUMBER CHECK(Suma>0),
    Data_Semnarii DATE);


--9. Componente:  –Creare tabel Componente
CREATE TABLE Componente(
    ID_Componenta NUMBER PRIMARY KEY,
    Nume_Componenta VARCHAR2(50) NOT NULL,
    ID_Vehicul NUMBER REFERENCES Vehicule_Spatiale(ID_Vehicul),
    Cost NUMBER CHECK(Cost>0));

--10. Stocuri: –Creare tabel Stocuri
CREATE TABLE Stocuri(
    ID_Stoc NUMBER PRIMARY KEY,
    ID_Componenta NUMBER REFERENCES Componente(ID_Componenta),
    Cantitate NUMBER CHECK (Cantitate>=0),
    Locatie VARCHAR2(50));

--11.Angajati Proiecte:  –Creare tabel Proiecte
CREATE TABLE Angajati_Proiecte (
    ID_Angajat NUMBER NOT NULL REFERENCES Angajati(ID_Angajat),
    ID_Proiect NUMBER NOT NULL REFERENCES Proiecte(ID_Proiect),
    Rol VARCHAR2(50),
    Ore_Lucrate NUMBER,
    PRIMARY KEY (ID_Angajat, ID_Proiect));

--12. Users: –Creare tabel Users
CREATE TABLE Users (
    ID_User NUMBER PRIMARY KEY,
    Nume_Utilizator VARCHAR2(50) NOT NULL,
    Parola VARCHAR2(255) NOT NULL,
    Rol VARCHAR2(50) CHECK (Rol IN ('Manager', 'Angajat', 'Administrator')) NOT NULL,
    Email VARCHAR2(100),
    Data_Inregistrare DATE DEFAULT SYSDATE);
--modificare atribut email ->adaug restricie de unicitate pentru email
ALTER TABLE Users ADD CONSTRAINT UC_Email UNIQUE (Email);-

--------------------------------------------------------------------------Actualizarea structurii tabelelor si modificarea restrictiilor de integritate -----------------------------------------------
--modificare atribut email ->adaug restricie de unicitate pentru email
ALTER TABLE Users ADD CONSTRAINT UC_Email UNIQUE (Email);
--ADAUGA RELATIE INTRE ANGAJATI SI USERS PENTRU CONTURI UTLILIZATOR
   ALTER TABLE Angajati ADD ID_User NUMBER;
ALTER TABLE Angajati ADD CONSTRAINT FK_Angajati_User FOREIGN KEY (ID_User) REFERENCES Users(ID_User);
-- modificare tabel angajati -> adauga coloana noua pentru numarul de telefon al angajatilor
ALTER TABLE Angajati 
ADD Telefon VARCHAR2(10);

--MODIFICA/EXTINDE DIMENSIUNEA DENUMIRII VEHICULULUI SPATIAL
ALTER TABLE Vehicule_Spatiale
MODIFY Nume_Vehicul VARCHAR2(75);
--ADAUGA COLOANA NOUA PENTRU FEEDBACK CLIENTI
ALTER TABLE Clienti 
ADD FEEDBACK VARCHAR(25);

--STERGERE COLOANA FEEDBACK CLIENTI
ALTER TABLE Clienti
DROP COLUMN FEEDBACK;

--CONSTRANGERE PENTRU CA SALARIUL ANGAJATILOR SA FIE MAI MARE DE 2500
ALTER TABLE Angajati
ADD CONSTRAINT CK_Salariu CHECK (Salariu>2500);
--constrangere pentru vehicule incat sa nu se repete
ALTER TABLE Vehicule_Spatiale
ADD CONSTRAINT UQ_TipVehicul UNIQUE (Tip_Vehicul);
--adauga un vehicul spatial ce are id-ul egal cu 301
INSERT INTO Vehicule_Spatiale (ID_Vehicul, Nume_Vehicul, Tip_Vehicul, Data_Construirii, ID_Lansare)
Values(301, 'Apollo 11', 'Racheta', TO_DATE('2023-11-11', 'YYYY-MM-DD'),NULL);
--adauga o componenta pentru un vehicul spatial
INSERT INTO Componente (ID_Componenta, Nume_Componenta, ID_Vehicul, Cost)
Values (501, 'Panou Solar', 301, 100000);
select from Componente;
--adauga in stocul cu id-ul 601, 25 de bucati din componenta cu id-ul 501, aflandu-se in depozitul central
INSERT INTO Stocuri (ID_Stoc, ID_Componenta, Cantitate, Locatie)
VALUES (601, 501, 25, 'Depozit Central');

