------------------------------------------------------------------------------------Actualizarea înregistrărilor------------------------------------------------------------------------------------
--1.actualizeaza numarul de ore lucrate pentru angajatul cu ID-ul 4 in proiectul cu ID-ul 7 la 1200
UPDATE Angajati_Proiecte
SET Ore_Lucrate=1200
WHERE ID_Angajat=4 AND ID_Proiect=7;
--2.modifica statusul proiectelor incepute in 2024 cu 'in progres'
UPDATE Proiecte
SET Status_Proiect = 'In progres'
WHERE Data_Inceput >= TO_DATE('2024-01-01', 'YYYY-MM-DD') 
  AND Data_Inceput < TO_DATE('2025-01-01', 'YYYY-MM-DD');
--3.modifica rolul angajatului cu ID-ul 2 in proiectul cu ID-ul 2 in "Coordonator"
UPDATE Angajati_Proiecte
SET Rol='Coordonator'
WHERE ID_Angajat=2 and ID_Proiect=2;
--4. adauga 100 de ore lucrate pentru angajatii care au lucrat mai mult de 1000 de ore in orice proiect
UPDATE Angajati_Proiecte
SET Ore_Lucrate=Ore_Lucrate+100
WHERE Ore_Lucrate>1000;

--5.modifica rolul angajatului cu id-ul 3 la 'Consultant' in proiectul cu id-ul 8, pentru proiectele care sunt in progres
UPDATE Angajati_Proiecte
SET Rol='Consultant'
WHERE ID_Angajat=3 AND ID_Proiect=8
AND ID_Proiect IN(SELECT ID_Proiect FROM Proiecte WHERE Status_Proiect='In progres');
Ștergerea și recuperarea unei tabele 
-----------------------------------------------------------------------Creare sau alegere tabelă ce urmează a fi ștearsă----------------------------------------------
CREATE TABLE Satelit(
    ID_Satelit NUMBER PRIMARY KEY,
    Nume_Satelit VARCHAR2(100) NOT NULL,
    Locatie_Lansare VARCHAR2(50),
    Stare_Actuala VARCHAR(100)
--Ștergere tabelă
DROP TABLE Satelit;
--Recuperare tabelă
FLASHBACK TABLE Satelit TO BEFORE DROP;
--------------------------------------------------------------------------------------Exemple de interogări variate (SELECT)-------------------------------------------------
-- Operatori de comparatie
--1. AFISEAZA ANGAJATII CARE AU SALARIUL MAIMARE DE 5000 SI S-AU ANGAJAT DUPA 2021
SELECT Nume, Prenume, Salariu, Data_Angajarii
FROM Angajati
WHERE Salariu>5000 AND Data_Angajarii>TO_DATE('2021-01-01', 'YYYY-MM-DD');
--2. AFISEAZA PROIECTELE CU BUGETUL PESTE 10 MILIOANE U.M.
SELECT Nume_Proiect, Buget 
FROM Proiecte 
WHERE Buget > 10000000;
--3.AFISEAZA LANSARILE CE AU AVUT LOC INTRE 1 MARTIE 2024 SI 30 IUNIE 2024
SELECT ID_Lansare, Data_Lansare
FROM Lansari
WHERE Data_Lansare BETWEEN TO_DATE('2024-03-01','YYYY-MM-DD') AND TO_DATE('2024-06-30','YYYY-MM-DD');
-- 4. AFISEAZA ANGAJATII SI DEPARTAMENTELE IN CARE LUCREAZA
SELECT a.Nume, a.Prenume, d.Nume_Departament 
FROM Angajati a
JOIN Departamente d ON a.ID_Departament = d.ID_Departament;
-- 5. AFISEAZA VEHICULELE SPATIALE SI PROIECTELE ASOCIATE
SELECT v.Nume_Vehicul, p.Nume_Proiect
FROM Vehicule_Spatiale v
JOIN Lansari l ON v.ID_Lansare = l.ID_Lansare
JOIN Proiecte p ON l.ID_Lansare = p.ID_Proiect;
-- 6. AFISEAZA CLIENTII SI PROIECTELE PENTRU CARE AU SEMNAT CONTRACTE, CAT SI VALOAREA PROIECTULUI RESPECTIV
SELECT c.Nume_Client, p.Nume_proiect, co.Suma
FROM Clienti c
JOIN Contracte co ON c.ID_Client = co.ID_Client
JOIN Proiecte p ON co.ID_Proiect = p.ID_Proiect;
-- 7. SUMA TOTALA A SALARIILOR IN FIECARE DEPARTAMENT
SELECT d.Nume_Departament, SUM(a.Salariu) AS Total_Salariu
FROM Angajati a
JOIN Departamente d ON a.ID_Departament = d.ID_Departament
GROUP BY d.Nume_Departament;
-- 8. MEDIA BUGETULUI PROIECTELOR
SELECT AVG(Buget) AS Media_Buget
FROM Proiecte;
-- 9. NUMARUL DE COMPONENTE IN FIECARE VEHICUL
SELECT v.Nume_Vehicul, COUNT(c.ID_Componenta) AS Numar_Componente
FROM Vehicule_Spatiale v
JOIN Componente c ON v.ID_Vehicul = c.ID_Vehicul
GROUP BY v.Nume_Vehicul;
-- 10. CALCUL VECHIME ANGAJATI
SELECT Nume, Prenume, ROUND(MONTHS_BETWEEN(SYSDATE, Data_Angajarii) / 12, 1) AS Ani_Experienta
FROM Angajati;
-- 11. AFISEAZA NUMELE PROIECTELOR IN ALL CAPS
SELECT UPPER(Nume_Proiect) AS Nume_Proiect_Upper
FROM Proiecte;
-- 12. AFISEAZA ZIUA SAPTAMANII PENTRU FIECARE LANSARE
SELECT ID_Lansare, TO_CHAR(Data_Lansare, 'Day') AS Ziua_Saptamanii
FROM Lansari;
-- 13. CLASIFICA SALARIILE ANGAJATILOR BY MEDIU, MIC, RIDICAT
SELECT Nume, Prenume, Salariu,
       CASE 
         WHEN Salariu > 8000 THEN 'Ridicat'
         WHEN Salariu BETWEEN 5000 AND 8000 THEN 'Mediu'
         ELSE 'Mic'
       END AS Clasa_Salariu
FROM Angajati;
-- 14. CLASIFICARE STARE PROIECTE IN FUNCTIE DE BUGET: IN PROGRES, IN DERULARE, FINALIZAT, COMPLET, NECUNOSCUT
SELECT Nume_Proiect, 
       DECODE(Status_Proiect, 
              'In progres', 'Proiect în derulare', 
              'Finalizat', 'Proiect complet',
              'Necunoscut') AS Status
FROM Proiecte;
-- 15. AFISEAZA PROIECTELE CARE NU AU LANSARI ASOCIATE
SELECT Nume_Proiect 
FROM Proiecte
MINUS
SELECT DISTINCT p.Nume_Proiect
FROM Proiecte p
JOIN Lansari l ON p.ID_Proiect = l.ID_Proiect;
SELECT Nume_Proiect AS PROIECTE
FROM Proiecte
WHERE Buget > 10000000
UNION
SELECT Nume_Proiect 
FROM Proiecte
WHERE Status_Proiect = 'In progres';
-- 17. AFISARE ANGAJATI CARE LUCREAZA LA CEL PUTIN DOUA PROIECTE DIFERITE
SELECT ID_Angajat 
FROM Angajati_Proiecte
GROUP BY ID_Angajat
HAVING COUNT(DISTINCT ID_Proiect) > 1
INTERSECT
SELECT ID_Angajat
FROM Angajati;
-- 18. AFISARE ANGAJATI CARE LUCREAZA IN PROIECTE CU BUGET PESTE 10 MILIOANE
SELECT Nume, Prenume 
FROM Angajati a
WHERE ID_Angajat IN (
    SELECT DISTINCT ID_Angajat
    FROM Angajati_Proiecte ap
    JOIN Proiecte p ON ap.ID_Proiect = p.ID_Proiect
    WHERE p.Buget > 10000000);
-- 19. AFISARE VEHICULE ASOCIATE CU CEL MAI SCUMP PROIECT
SELECT Nume_Vehicul 
FROM Vehicule_Spatiale v
WHERE ID_Lansare IN (
    SELECT ID_Lansare 
    FROM Lansari
    WHERE ID_Proiect = (
        SELECT ID_Proiect 
        FROM Proiecte 
        WHERE Buget = (SELECT MAX(Buget) FROM Proiecte)
    )
);
-- 20. CALCULATI MEDIA COSTULUI COMPONENTELOR DIN FIECARE VEHICUL
SELECT ID_Vehicul, AVG(Cost) AS Media_Cost
FROM Componente
WHERE Cost > (SELECT AVG(Cost) FROM Componente)
GROUP BY ID_Vehicul;
-- 21. TRANSFORMA NUMELE VEHICULELOR IN LITERE MICI
SELECT LOWER(Nume_Vehicul) AS Nume_Lit_Mici 
FROM Vehicule_Spatiale;
-- 22. EXTRAGE PRIMELE 3 CARACTERE DIN NUMELE CLIENTILOR
SELECT SUBSTR(Nume_Client, 1, 3) AS Prefix_Client 
FROM Clienti;
-- 23. PRECIZATI POZITIA PRIMEI APARITII A LITEREI 'E' IN NUMELE PROIECTELOR
SELECT INSTR(Nume_Proiect, 'e') AS Pozitie_Litera_E 
FROM Proiecte;
-- 24. ROTUNJITI COSTUL LANSARILOR
SELECT ROUND(Cost, -3) AS Cost_Rotunjit 
FROM Lansari;
-- 25. CARE ESTE DATA URMATOAREI ZILE DE LUNI PENTRU FIECARE LANSARE?
SELECT ID_Lansare, NEXT_DAY(Data_Lansare, 'MONDAY') AS Urmatoarea_Luni 
FROM Lansari;
-- 26. CARE ESTE DIFERENTA DINTRE DATA INCEPERII SI DATA SFARSITULUI PROIECTELOR?
SELECT Nume_Proiect, MONTHS_BETWEEN(Data_Sfarsit, Data_Inceput) AS Diferenta_Luni 
FROM Proiecte;
-- 27. CARE ESTE ULTIMA ZI A LUNII PENTRU FIECARE LANSARE?
SELECT ID_Lansare, LAST_DAY(Data_Lansare) AS Ultima_Zi_Luna 
FROM Lansari;
-- 28. CARE ESTE COSTUL TOTAL AL COMPONENTELOR PENTRU FIECARE VEHICUL?
SELECT ID_Vehicul, SUM(Cost) AS Total_Cost 
FROM Componente 
GROUP BY ID_Vehicul;
-- 29. CARE ESTE NUMARUL DE LANSARI PENTRU FIECARE PROIECT?
SELECT ID_Proiect, COUNT(ID_Lansare) AS Nr_Lansari 
FROM Lansari 
GROUP BY ID_Proiect;
-- 30. MEDIA SALARIILOR PENTRU FIECARE DEPARTAMENT CARE ESTE PESTE 6000 
SELECT ID_Departament, AVG(Salariu) AS Media_Salariu 
FROM Angajati 
GROUP BY ID_Departament 
HAVING AVG(Salariu) > 6000;
-- 31. PROIECTELE IN CARE FIECARE SUB-PROIECT ESTE SUBORDONAT PROIECTULUI PRINCIPAL
SELECT ID_Proiect, Nume_Proiect, LEVEL, CONNECT_BY_ISLEAF AS Este_Frunza 
FROM Proiecte 
CONNECT BY PRIOR ID_Proiect = ID_Proiect - 1 
START WITH ID_Proiect = 1;
-- 32. AFISARE PROIECTE CARE AU BUGETUL PESTE MEDIA TOTALA A BUGETELOR
SELECT Nume_Proiect, Buget 
FROM Proiecte 
WHERE Buget > (SELECT AVG(Buget) FROM Proiecte);
-- 33. CARE ESTE LUNGIMEA NUMELUI FIECARUI PROIECT?
SELECT Nume_Proiect, LENGTH(Nume_Proiect) AS Lungime_Nume 
FROM Proiecte;
-- 34. CONCATENATI NUMELE CLIENTILOR CU NUMELE TARII DE ORIGINE
SELECT CONCAT(Nume_Client, ', ' || Tara) AS Client_Tara 
FROM Clienti;
-- 35. EXTRAGETI ANUL LANSARII PENTRU FIECARE VEHICUL LANSAT
SELECT ID_Lansare, EXTRACT(YEAR FROM Data_Lansare) AS Anul_Lansarii 
FROM Lansari;

---------------------------------------------------------------------------------Gestiunea altor obiecte ale bazei de date: vederi, indecsi, sinonime-----------------------------------------------------
--VIEW PENTRU A URMARI MAI USOR ANGAJATII SI DEPARTAMENTELE IN CARE LUCREAZA
CREATE VIEW Angajati_Departamente_View AS
SELECT a.Nume, a.Prenume, d.Nume_Departament
FROM Angajati a
JOIN Departamente d ON a.ID_Departament = d.ID_Departament;
SELECT * FROM Angajati_Departamente_View;
--VIEW PENTRU A URMARI MAI USOR PROIECTELE SI COSTURILE LOR DE LANSARE
CREATE VIEW Proiecte_Lansari_View AS
SELECT p.Nume_Proiect, p.Buget, l.Data_Lansare, l.Cost
FROM Proiecte p
JOIN Lansari l ON p.ID_Proiect = l.ID_Proiect;
SELECT * FROM Proiecte_Lansari_View;
--INDEX PE COLOANA SALARIU DIN ANGAJATI PT A ACCELERA CAUTARILE
CREATE INDEX idx_salariu_angajati ON Angajati (Salariu);
--INDEX COMPUS DIN COLOANELE ID_DEPARTAMENT SI ID_DUNCTIE DIN ANGAJATI
CREATE INDEX idx_departament_functie ON Angajati (ID_Departament, ID_Functie);

SELECT * FROM USER_INDEXES;
--STERGERE INDEX
DROP INDEX idx_salariu_angajati;

--SINONIM PENTRU TABELELE: ANGAJATI, PROIECTE_LANSARI_VIEW
CREATE SYNONYM Angajati_Syn FOR Angajati;
CREATE SYNONYM Proiecte_Lansari_View_Syn FOR Proiecte_Lansari_View;
SELECT * FROM Angajati_Syn;
SELECT * FROM Proiecte_Lansari_View_Syn;










